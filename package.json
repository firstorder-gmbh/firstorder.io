{
  "name": "firstorder",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/firstorder-gmbh/universal-starter.git"
  },
  "contributors": [
    "Philip Sultanescu <philip@firstorder.io>"
  ],
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "lint": "ng lint firstorder",
    "tslint": "tslint \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "htmllint": "htmllint \"src/**/*.html\"",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "test": "ng test",
    "test-debug": "ng test --single-run=false",
    "e2e": "ng e2e",
    "build-themes": "bash ./scripts/build-themes.sh",
    "build:client-and-server-bundles": "npm run build-themes && ng build --prod && ng run firstorder:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/cdk": "^6.3.1",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/material": "^6.3.1",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/platform-server": "^6.0.7",
    "@angular/router": "^6.0.7",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "angularfire2": "^5.0.0-rc.11",
    "core-js": "^2.5.7",
    "firebase": "^5.2.0",
    "hammerjs": "^2.0.8",
    "rxjs": "6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.1",
    "codelyzer": "^4.4.2",
    "cpy-cli": "^2.0.0",
    "htmllint-cli": "0.0.7",
    "express": "^4.16.3",
    "highlight.js": "^9.12.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.12",
    "stylelint": "^9.3.0",
    "stylelint-order": "^0.8.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.0.8",
    "ws": "^5.2.1",
    "xmlhttprequest": "^1.8.0"
  }
}
