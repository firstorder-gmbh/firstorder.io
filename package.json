{
  "name": "firstorder",
  "version": "0.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/firstorder-gmbh/universal-starter.git"
  },
  "contributors": [
    "Nicoleta Stancu <nico@togaimperial.com>",
    "Philip Sultanescu <philip@firstorder.io>"
  ],
  "engines": {
    "node": ">= 12.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "lint": "ng lint firstorder",
    "tslint": "tslint \"src/**/*.ts\" \"e2e/**/*.ts\" \"*.ts\"",
    "htmllint": "htmllint \"src/**/*.html\"",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "test": "ng test",
    "test-debug": "ng test --single-run=false",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e --no-webdriver-update",
    "build-themes": "bash ./scripts/build-themes.sh",
    "build:client-and-server-bundles": "npm run build-themes && ng build -c prod && ng run firstorder:server:prod",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/fire": "5.2.3",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/router": "8.2.14",
    "@nguniversal/common": "8.2.6",
    "@nguniversal/express-engine": "8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.4.7",
    "firebase": "7.6.1",
    "hammerjs": "2.0.8",
    "rxjs": "6.5.3",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "8.3.21",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.14",
    "codelyzer": "5.2.1",
    "express": "4.17.1",
    "highlight.js": "9.17.1",
    "htmllint-cli": "0.0.7",
    "http-server": "0.12.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "node-sass": "^4.13.1",
    "pre-commit": "1.2.2",
    "protractor": "5.4.2",
    "reflect-metadata": "0.1.13",
    "stylelint": "12.0.1",
    "stylelint-order": "4.0.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-eslint-rules": "5.4.0",
