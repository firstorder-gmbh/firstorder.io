{
  /** 
  * Use IntelliSense to learn about possible attributes.
  * Hover to view descriptions of existing attributes.
  *
  * To start debugging an angular-cli project follow these steps
  * 0. Install the Debugger for Chrome Extension
  *
  * 1. Put a breakpoint in your code
  * 2. Run "npm run start" command in the terminal
  * 3. Goto Debug, select the "npm run start" configuration and press Start Debugging
  *
  * 4. To debug unit tests use "npm run test-debug", use fit() instead of it() to only debug specific tests
  * 5. To debug e2e tests use "npm run e2e", refresh the web page if the breakpoint doesn't get hit
  *
  * For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  * for angular-cli debugging, visit: https://code.visualstudio.com/docs/nodejs/angular-tutorial
  */
  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm run start",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "npm run test-debug",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:9876/debug.html",
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "npm run e2e",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
      "protocol": "inspector",
      "args": ["${workspaceRoot}/protractor.conf.js"]
    }
  ]
}
